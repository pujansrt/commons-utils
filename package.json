{
  "name": "commons-utils",
  "version": "2.2.0",
  "description": "Common Utilities",
  "main": "dist/commons.utils.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --staements 0 --branches 0 --functions 0 --lines 0",
    "cover": "nyc npm t",
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && tsc --project ./ --downlevelIteration",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pujansrt/commons-utils"
  },
  "keywords": [
    "typescript",
    "javascript",
    "node",
    "common",
    "utilities",
    "array",
    "es6"
  ],
  "author": "Pujan Srivastava <pujansrt@gmail.com> (http://pujansrt.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pujansrt/commons-utils/issues"
  },
  "homepage": "https://github.com/pujansrt/commons-utils",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "commitizen": "2.9.4",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "2.0.0",
    "jest": "^23.5.0",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.0",
    "nyc": "10.0.0",
    "rimraf": "2.5.4",
    "semantic-release": "6.3.2",
    "systemjs": "^0.21.4",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "typescript": "2.9.2",
    "uglify-js": "2.7.5",
    "webpack": "1.14.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "**/*.{ts}"
    ],
    "coverageDirectory": "./test-coverage",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|js|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ]
  }
}
