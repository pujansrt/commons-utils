{
  "name": "commons-utils",
  "version": "0.0.0-semantically-released",
  "description": "Common Utilities",
  "main": "dist/commons-utils.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --staements 100 --branches 50 --functions 100 --lines 50",
    "cover": "nyc npm t",
    "test": "mocha src/commons-utils.test.js --compilers js:babel-register",
    "test:w": "mocha src/commons-utils.test.js --compilers js:babel-register -w",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "buildes5": "tsc --target ES5",
    "build:ts": "tsc",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pujansrt/commons-utils"
  },
  "keywords": [
    "typescript",
    "javascript",
    "node",
    "common",
    "utilities",
    "array",
    "es6"
  ],
  "author": "Pujan Srivastava <pujansrt@gmail.com> (http://pujansrt.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pujansrt/commons-utils/issues"
  },
  "homepage": "https://github.com/pujansrt/commons-utils",
  "devDependencies": {
    "typescript": "2.1.4",
    "babel-cli": "6.18.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "commitizen": "2.9.4",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "2.0.0",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.0",
    "nyc": "10.0.0",
    "rimraf": "2.5.4",
    "semantic-release": "6.3.2",
    "webpack": "1.14.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
